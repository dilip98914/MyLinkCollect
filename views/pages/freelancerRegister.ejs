<%- include('../partials/header') %>
<%- include('../partials/navbar.ejs') %>
<div class="login-inner loginWrp">
  <div class="container">
    <div class="heading-register text-center"> Freelancer&nbsp;
      <span> Sign Up </span>
    </div>
    <div class="row login">
      <div class="col-xs-12 col-md-6 col-sm-6">
        <!-- <div class="btn-gmail text-center">
          <a href="/google">
            <img src="/ssrsystem/images/longsignup.jpg" height="40px" width="100%"> </a>
        </div>
        <div class="padd-google">
          <p class="google-sso-separator m-lg-top-bottom"><span>or</span></p>
        </div> -->
        <div class="conForm">
          <form [formGroup]="companyForm">
            <input type="hidden" id="countryname" value="">
            <input type="hidden" id="phonecode" value="">
            <div class="input-wrap row">
              <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                <input type="text" placeholder="First Name*" class="form-control" id="fname" maxlength="50" onchange="fieldcheck('fnameerror','fname');" onkeyup="fieldcheck('fnameerror','fname');">
                <small class="text-danger texthide" id="fnameerror">
                  First name is required*
                </small>
              </div>
              <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 pad-top-15">
                <input type="text" placeholder="Last Name*" class="form-control" id="lname" maxlength="50" onchange="fieldcheck('lnameerror','lname')" onkeyup="fieldcheck('lnameerror','lname')">
                <small class="text-danger texthide" id="lnameerror">
                  Last name is required*
                </small>
              </div>
            </div>
            <div class="input-wrap row">
              <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 pad-top-15">
                <div class="dropdown drop-dwn pointer">
                  <div class="dropdown-toggle dropcode" data-toggle="dropdown">
                    <span id="sltpc"> Select Phone Code </span>
                    <span class="caret drop-caret"></span>
                  </div>
                  <ul class="dropdown-menu" id="myDropdown">
                    <li>
                      <div class="search-div-reg">
                        <input type="text" class="search-text" placeholder="search code or country" onInput="searchcountry(this.value);" />
                      </div>
                    </li>
                    <ul class="dropdown-inner">
                    </ul>
                  </ul>
                </div>
              </div>
              <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 pad-top-15">
                <input type="text" placeholder="Mobile*" class="form-control" id="phone" maxlength="12" onchange="fieldcheck('phoneerror','phone')" onkeyup="fieldcheck('phoneerror','phone')" onkeypress="return isNumber(event)">
                <small class="text-danger texthide" id="phoneerror">
                  Mobile is required*</small>
              </div>
            </div>
            <div class="input-wrap row">
              <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 pad-top-15">
                <input type="text" placeholder="Email*" class="form-control" id="email" maxlength="100" onchange="checkmailvalid(this.value)" onchange="fieldcheck('emailerror','email')" onkeyup="fieldcheck('emailerror','email')">
                <small class="text-danger texthide" id="emailerror">
                  Email is required*
                </small>
                <small id="bemailstt" class="text-danger texthide">
                  Please enter only business email*
                </small>
              </div>
              <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 pad-top-15">
                <input type="password" autocomplete="password" placeholder="password*" class="form-control" id="password" onchange="fieldcheck('passworderror','password')" onkeyup="fieldcheck('passworderror','password')">
                <div class="toggle-password" onclick="changepwd()" rel="hide">
                  <div class="view-pwd"> <img src="images/view-pwd.png" /> </div>
                  <div class="hide-pwd"> <img src="images/hide-pwd.png" /> </div>
                </div>
                <small class="text-danger texthide" id="passworderror">
                  Password must be of at least 5 characters and not exceed 15 characters.
                </small>
              </div>
            </div>
            <div class="input-wrap row">
              <div id="otherlocation" class="col-xs-12 col-sm-12 col-md-6 col-lg-6 pad-top-15">
                <input type="text" placeholder="Type City Name" class="form-control" id="locationselect" onkeyup="get_city(this.value)">
                <span class="city_loader" style="display: none;">
                  <img src="images/loader.gif" />
                </span>
                <div id="myDropdown" class="dropdown-content displayblock" style="display: none">
                </div>
              </div>
              <div id="otherlocation2" class="col-xs-12 col-sm-12 col-md-6 col-lg-6 pad-top-15" style="display:  none;">
                <input type="text" placeholder="Enter City Name" class="form-control" id="location">
                <button type="button" class="btn btn-reset" onclick="resetdata()">Reset</button>
                <span id="loderstt" class="city_loader" style="display: none;">
                  <img src="images/loader.gif" />
                </span>
              </div>
              <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 margin-top-15 upload-div">
                <input type="text" class="form-control file-upload-text" disabled placeholder="Upload Resume*" />
                <div class="file-upload-btn">
                  Choose File
                  <input type="file" id="resumeid" class="file-upload" name="myFile" accept=".docx,.pdf,.doc" onchange="resumeupload()" />
                </div>
                <div class="progress-div" style="display: none;">
                  <span title="Reset File" onclick="resetfile()" class="reset-div">
                    <span class="close-file-btn"> X </span>
                    <span class="small-loader" style="display: none;">
                      <img src="images/loader.gif" width="25px" /> </span>
                  </span>
                  <div class="progress">
                    <div class="progress-bar"></div>
                  </div>
                </div>
              </div>
            </div>
            <div class="input-wrap row">
              <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 pad-top-15">
                <div class="skill-test">
                  <span class="main-top">
                  </span>
                  <span> <input type="text" id="skillid" placeholder="Write key skills separated with comma*" class="skills-write text-width-200" maxlength="50" onkeyup="get_skill_data(this.value)">
                  </span>
                </div>
                <div class="dropdown-menu drop2">
                </div>
              </div>
            </div>
            <div class="input-wrap row">
              <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 pad-top-15">
                <div class="checkbox-success">
                  <input id="agree" type="checkbox" name="agree">
                  <label for="agree">
                    &nbsp;I agree to
                    <a href="terms-condition.html" target="_blank">&nbsp;Terms & Conditions.</a>
                  </label>
                </div>
              </div>
            </div>
            <div class="sub-btn">
              <button id="registerbtn" disabled="disabled" type="button" class="btn-theme-100" onclick="registerSubmit()">Sign up
                <span class="regloader" style="display: none;">
                  <img src="images/loginloader.gif" /> </span>
              </button>
              <button id="loginstt2" style="width: 100%;display: none;" type="button" class="btn btn-gray">
                Please Wait to view Profile
                <img src="images/loginloader.gif" />
              </button>
            </div>
            <input type="hidden" id="filename" value="" />
            <input type="hidden" id="originalimage" value="" />

          </form>
        </div>
      </div>
      <div class="col-xs-12 col-md-6 col-sm-6">
        <div class="btn-gmail text-center">

        </div>
        <h6>Why will you choose</h6>
        <p> Get or Post Job/Project. Get connected with other Freelancers. Create your Virtual Team. Get connected
          with Client
          and other freelancers without any restriction and commission</p>
        <div class="inner-test">
          <div id="myCarousel" class="carousel slide" data-ride="carousel">
            <ol class="carousel-indicators">
              <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
              <li data-target="#myCarousel" data-slide-to="1"></li>
            </ol>
            <div class="carousel-inner">
              <div class="item active">
                <div class="testimonial-block">
                  <div class="row">
                    <div class="col-md-12">
                      <div class="testimonial-content">
                        <div>"OnBenchMark has provided me an opportunity to directly connect with many clients
                          without charging any commission.
                          I was always looking for such website which can connect me to global clients. Its hard to
                          believe they donâ€™t charge any commission as they are charging clients but not the
                          freelancer. I am happy to be a part of OnBenchMark"</div>
                      </div>
                      <div class="testimonial-photo">
                        <img src="images/harry.jpg" width="56" height="56" style="border: 2px solid #8a8f8c;">
                        <span class="testimonial-company">
                          <span class="testimonial-name">Harry</span>Video/VFX Producer
                        </span>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <div class="item">
                <div class="testimonial-block">
                  <div class="row">
                    <div class="col-md-12">
                      <div class="testimonial-content">
                        <div>"I am happy to be associated with OnBenchMark as it provides me much needed freedom as
                          a freelancer. I got some very good projects through them without worrying about paying
                          high commission unlike other top freelancing website.
                          I highly recommend OnBenchMark for its trusted services at no extra cost"</div>
                      </div>
                      <div class="testimonial-photo">
                        <img src="images/ankit.jpg" width="56" height="56" style="border: 2px solid #8a8f8c;">
                        <span class="testimonial-company">
                          <span class="testimonial-name">Ankit Kumar Singh</span>Graphic Designer
                        </span>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <a class="left carousel-control" href="#myCarousel" data-slide="prev">
              <img src="images/left-chevron.png" alt="left chevron" />
              <span class="sr-only">Previous</span>
            </a>
            <a class="right carousel-control" href="#myCarousel" data-slide="next">
              <img src="images/right-chevron.png" alt="right chevron" />
              <span class="sr-only">Next</span>
            </a>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<%- include('../partials/footer.ejs') %>
<%- include('../partials/bottom.ejs') %>